                  <xs:restriction base="xs:double"/>
                </xs:simpleType>
              </xs:restriction>
            </xs:simpleContent>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:sequence>
  </xs:group>
  <xs:attributeGroup name="foo">
    <xs:attribute name="foo" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:double"/>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
</xs:schema>
