  <xs:import namespace="http://www.example.com/2" schemaLocation="e2.xsd"/>
  <xs:element name="x">
    <xs:complexType>
      <xs:sequence>
        <xs:group ref="e2:anon0"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
