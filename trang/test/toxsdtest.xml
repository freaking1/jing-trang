            </xs:restriction>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:group>
  <xs:simpleType name="bar">
    <xs:union>
      <xs:simpleType>
        <xs:restriction base="xs:int"/>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="xs:boolean"/>
